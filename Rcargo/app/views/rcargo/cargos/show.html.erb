<div id="cargo_show" >
    <%cache "cargoshow"+params[:id] do%>
     <dl class="dl-horizontal">
       <dt>货物名称</dt>  <dd><%=@cargo.name%><dd>
       <dt>出发城市</dt> 
       <dd><%=@cargo.fcityn%><dd>
       <dt>到达城市</dt> 
       <dd><%=@cargo.tcityn%><dd>
       <dt>货物大类</dt> 
       <dd><%=@cargo.big_cate%><dd>
       <dt>货物重量</dt> 
       <dd><%=@cargo.weight%><dd>
       <dt>有效期</dt> 
       <dd><%=@cargo.expire%><dd>
       <dt>备注</dt> 
       <dd><%=@cargo.comments%><dd>
      </dl>
    <%end%>

      <%#=render "jubaos/jubaoform"%>
</div>

